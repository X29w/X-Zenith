{
	"expo": {
		"name": "X-Zenith",
		"slug": "X-Zenith",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"scheme": "x-zenith",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.x29.XZenith",
			"infoPlist": {
				"UIBackgroundModes": ["audio"]
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#000"
			},
			"package": "com.x29.XZenith"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "805aa8fc-2353-447c-8c5e-116340e30b5a"
			}
		}
	}
}
